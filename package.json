{
    "name": "@oasislabs/parcel-client",
    "version": "0.1.0",
    "license": "Apache-2.0",
    "author": "Oasis Labs <feedback@oasislabs.com>",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "tsc -b",
        "lint": "xo",
        "test": "jest",
        "coverage": "jest --coverage",
        "doc": "typedoc",
        "prepublishOnly": "yarn build"
    },
    "jest": {
        "coveragePathIgnorePatterns": [
            "test/*"
        ],
        "coverageReporters": [
            "lcov",
            "text",
            "cobertura"
        ],
        "moduleFileExtensions": [
            "js",
            "ts"
        ],
        "transform": {
            "\\.ts$": "ts-jest"
        },
        "testEnvironment": "node",
        "testRegex": ".*\\.spec\\.ts$"
    },
    "devDependencies": {
        "@apidevtools/swagger-parser": "^10.0.2",
        "@types/jest": "^26.0.9",
        "@types/node": "^14.0.20",
        "@types/openapi-sampler": "^1.0.0",
        "@types/uuid": "^8.3.0",
        "ajv": "^6.12.5",
        "jest": "^26.2.2",
        "jsonwebtoken": "^8.5.1",
        "jwk-to-pem": "^2.0.4",
        "nock": "^13.0.4",
        "openapi-types": "^7.0.1",
        "ts-jest": "^26.1.4",
        "typedoc": "^0.18.0",
        "typescript": "^3.9.7",
        "uuid": "^8.3.0",
        "xo": "^0.33.1"
    },
    "dependencies": {
        "@types/readable-stream": "^2.3.9",
        "axios": "^0.19.2",
        "eventemitter3": "^4.0.4",
        "form-data": "^3.0.0",
        "param-case": "^3.0.3",
        "readable-stream": "^3.6.0"
    }
}
